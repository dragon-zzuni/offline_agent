{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "serena-mcp-server",
        "--enable-web-dashboard",
        "true",
        "start-mcp-server"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    }
  }
}
